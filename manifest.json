{
  "name": "YouTube date display fixer",
  "description": "Fix YouTube date display",
  "version": "1.0.2",
  "manifest_version": 3,
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
